{
  "_version": "1.76.0",
  "sap.app": {
    "id": "sapui5finaltask",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "dataSources": {
      "odataV2": {
        "uri": "/mockServerV2/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadataV2.xml"
        }
      }
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "92c1a42f-771e-4fed-a1b0-81d53293fd32"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.98.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {} 
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sapui5finaltask.i18n.i18n"
        }
      },
      "": {
        "dataSource": "odataV2",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "sapui5finaltask.view",
        "controlId": "layout",
        "controlAggregation": "beginColumnPages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": "master"
        },
        {
          "pattern": "Products/{objectId}/:?query:",
          "name": "object",
          "target": ["master", "object"]
        }
      ],
      "targets": {
        "master": {
          "id": "master",
          "name": "Master",
          "level": 1
        },
        "object": {
          "id": "detail",
          "name": "Detail",
          "level": 2,
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "sapui5finaltask.view.App",
      "type": "XML",
      "id": "app",
      "async": true
    }
  }
}
